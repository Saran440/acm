<odoo>

    <report
        id="action_report_rental_collection"
        model="rental.collect.report.wizard"
        string="Rental Collection Form"
        report_type="qweb-pdf"
        name="acm.report_rental_collection"
        file="acm.report_rental_collection"
        print_report_name="'Rental Collection'"
        paperformat="acm.report_paperformat_rental_collect"
        menu="False"/>

    <report
        id="action_report_payment_voucher"
        model="account.invoice"
        string="Voucher ACM"
        report_type="qweb-pdf"
        name="acm.report_payment_voucher"
        file="acm.report_payment_voucher"
        print_report_name="'Voucher_ACM - %s' %object.number"/>

    <report
        id="action_report_receipt_acm"
        model="account.invoice"
        string="Receipt ACM"
        report_type="qweb-pdf"
        name="acm.report_receipt_acm"
        file="acm.report_receipt_acm"
        print_report_name="'Receipt_ACM - %s' %object.number"
        paperformat="acm.report_paperformat_receipt_acm"/>

</odoo>
